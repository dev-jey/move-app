{
  "name": "tembea-frontend",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:staging": "ng serve -c=staging",
    "start:prod": "ng serve --prod",
    "build": "ng build",
    "build:staging": "ng build -c=staging",
    "build:prod": "ng build --prod",
    "test": "ng test --runInBand -w=1",
    "pretest": "npm run lint --fix",
    "lint": "ng lint --exclude 'src/**/*.json'",
    "e2e": "ng e2e",
    "cypress:open": "concurrently \"ng serve\" \"cypress open\""
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0-beta.5",
    "@angular/animations": "^7.2.2",
    "@angular/cdk": "^7.2.2",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/flex-layout": "^7.0.0-beta.24",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/material": "^7.2.2",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/pwa": "^0.13.8",
    "@angular/router": "~7.0.0",
    "@angular/service-worker": "~7.0.0",
    "@auth0/angular-jwt": "^2.1.0",
    "@bugsnag/core": "^6.0.0",
    "@bugsnag/js": "^6.0.0",
    "@bugsnag/plugin-angular": "^6.0.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "agm-direction": "^0.7.6",
    "core-js": "^2.5.4",
    "file-saver": "^2.0.1",
    "hammerjs": "^2.0.8",
    "moment": "^2.24.0",
    "rxjs": "~6.3.3",
    "rxjs-compat": "^6.3.3",
    "toastr": "^2.1.4",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-builders/jest": "^7.2.1",
    "@angular-devkit/build-angular": "^0.12.2",
    "@angular/cli": "~7.0.4",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@bahmutov/add-typescript-to-cypress": "^2.0.0",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/node": "~8.9.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "codelyzer": "~4.5.0",
    "commitplease": "^3.2.0",
    "concurrently": "^4.1.0",
    "cypress": "^3.1.4",
    "husky": "^2.4.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.2.1",
    "ng-lint-staged": "^0.1.6",
    "ngx-cypress-builder": "^1.0.2",
    "ts-node": "~7.0.0",
    "tslint": "^5.17.0",
    "typescript": "~3.1.1"
  },
  "commitplease": {
    "nohook": true,
    "style": "angular",
    "types": [
      "feat",
      "fix",
      "docs",
      "style",
      "refactor",
      "perf",
      "test",
      "chore"
    ],
    "scope": "\\S+.*"
  },
  "jest": {
    "coverageReporters": [
      "html",
      "text",
      "lcov"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "ignore": [],
    "linters": {
      "src/**/*.ts": [
        "ng-lint-staged lint --fix --",
        "git add"
      ]
    }
  }
}
